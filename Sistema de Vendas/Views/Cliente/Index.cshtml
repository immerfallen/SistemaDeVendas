<h3>Lista de Clientes</h3>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>CPF</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in (List<ClienteModel>)ViewBag.ListaClientes)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                    <td>@item.CPF_CNPJ</td>
                    <td>@item.Email</td>
                </tr>
            }
        }
    </tbody>
</table>

<button class="btn btn-block btn-primary" type="button">Cadastrar Cliente</button>